<UserControl x:Class="VesselTracking.Controls.PropertyControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:iDeal_silverlight_classes="clr-namespace:iDeal.silverlight.classes;assembly=iDeal.silverlight.5"
             xmlns:iDeal_silverlight_controls="clr-namespace:iDeal.silverlight.controls;assembly=iDeal.silverlight.5"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
             xmlns:vtClasses="clr-namespace:VesselTracking.Classes"
             Width="231"
             Height="310"
             d:DesignHeight="309"
             d:DesignWidth="231"
             mc:Ignorable="d">
    <UserControl.Resources>
        <!--
            <vtClasses:ConverterShipAndCargoForToolTip x:Key="ConverterShipAndCargoForToolTip" />
            <vtClasses:ConverterShipAndCargo x:Key="ConverterShipAndCargo" />
            <iDeal_silverlight_classes:ConverterDateTime x:Key="ConverterDateTime" />
        -->
        <Style x:Key="InfoLabel" TargetType="TextBlock">
            <Setter Property="Foreground" Value="#FF878787" />
            <Setter Property="FontSize" Value="9.333" />
            <Setter Property="Height" Value="14" />
            <Setter Property="Margin" Value="0" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="HorizontalAlignment" Value="Left" />
        </Style>
        <Style x:Key="InfoText" TargetType="TextBlock">
            <Setter Property="Foreground" Value="Yellow" />
            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect BlurRadius="2" ShadowDepth="2" />
                </Setter.Value>
            </Setter>
            <Setter Property="Height" Value="16" />
            <Setter Property="FontSize" Value="12" />
            <Setter Property="FontFamily" Value="Trebuchet MS" />
            <Setter Property="VerticalAlignment" Value="Bottom" />
        </Style>
    </UserControl.Resources>



    <Grid x:Name="LayoutRoot">
        <Border Margin="0,4,4,0"
                BorderThickness="1"
                CornerRadius="10">
            <Border.BorderBrush>
                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                    <GradientStop Color="#FF071621" />
                    <GradientStop Offset="1" Color="#FF103C5A" />
                </LinearGradientBrush>

            </Border.BorderBrush>
            <Border.Background>
                <SolidColorBrush Color="{StaticResource DarkBlue}" />
            </Border.Background>
        </Border>
        <Grid x:Name="closeGrid"
              Width="18"
              Height="18"
              HorizontalAlignment="Right"
              VerticalAlignment="Top"
              MouseEnter="closeGrid_MouseEnter"
              MouseLeave="closeGrid_MouseLeave"
              MouseLeftButtonUp="Grid_MouseLeftButtonUp">
            <Grid.Effect>
                <DropShadowEffect BlurRadius="14"
                                  Direction="232"
                                  ShadowDepth="2" />
            </Grid.Effect>
            <Ellipse Fill="Black"
                     MouseLeftButtonDown="Ellipse_MouseLeftButtonDown"
                     Stroke="#FF9B9B9B"
                     StrokeEndLineCap="Triangle"
                     StrokeLineJoin="Bevel"
                     StrokeStartLineCap="Triangle" />
            <Path Margin="4"
                  Data="M11.416672,4 L3.4583282,11.416666"
                  Fill="Black"
                  Stretch="Fill"
                  Stroke="White"
                  StrokeEndLineCap="Triangle"
                  StrokeLineJoin="Bevel"
                  StrokeStartLineCap="Triangle"
                  StrokeThickness="2"
                  UseLayoutRounding="False" />
            <Path Margin="4"
                  Data="M11.416672,4 L3.4583282,11.416666"
                  Fill="Black"
                  RenderTransformOrigin="0.5,0.5"
                  Stretch="Fill"
                  Stroke="White"
                  StrokeEndLineCap="Triangle"
                  StrokeLineJoin="Bevel"
                  StrokeStartLineCap="Triangle"
                  StrokeThickness="2"
                  UseLayoutRounding="False">
                <Path.RenderTransform>
                    <CompositeTransform ScaleX="-1" />
                </Path.RenderTransform>
            </Path>
        </Grid>
        <StackPanel x:Name="spMenu"
                    Height="31"
                    Margin="8,0,12,5"
                    VerticalAlignment="Bottom"
                    Orientation="Horizontal">
            <iDeal_silverlight_controls:CButton x:Name="btnLock"
                                                Width="28"
                                                Height="28"
                                                Margin="2,0,5,0"
                                                BackImage="/VesselTracking;component/Images/follow.png"
                                                Clicked="btnLock_Clicked"
                                                IsToggle="True"
                                                ToolTipService.ToolTip="Lock view to selected" Visibility="{Binding AISType, ConverterParameter=A\,B, Converter={StaticResource ConverterAISTypeVisibility}}" />
            <iDeal_silverlight_controls:CButton x:Name="btnTrackLine"
                                                Width="28"
                                                Height="28"
                                                Margin="3,0,5,0"
                                                BackImage="/VesselTracking;component/Images/1326534515_chart_line.png"
                                                Clicked="btnTrackLine_Clicked"
                                                ToolTipService.ToolTip="Show trackline for selected" Visibility="{Binding AISType, ConverterParameter=A\,B, Converter={StaticResource ConverterAISTypeVisibility}}"/>
            <iDeal_silverlight_controls:CButton x:Name="btnHistory_Copy"
                                                Width="28"
                                                Height="28"
                                                Margin="3,2,5,1"
                                                BackImage="/VesselTracking;component/Images/deselect.png"
                                                Clicked="btnTracklineRemove_Clicked"
                                                ToolTipService.ToolTip="Hide trackline for selected" Visibility="{Binding AISType, ConverterParameter=A\,B, Converter={StaticResource ConverterAISTypeVisibility}}"/>
            <iDeal_silverlight_controls:CButton x:Name="btnHistory"
                                                Width="28"
                                                Height="28"
                                                Margin="3,2,5,1"
                                                BackImage="/VesselTracking;component/Images/1326533559_clock.png"
                                                Clicked="btnHistory_Clicked"
                                                ToolTipService.ToolTip="History player for the selected" Visibility="{Binding AISType, ConverterParameter=A\,B, Converter={StaticResource ConverterAISTypeVisibility}}"/>
            <iDeal_silverlight_controls:CButton x:Name="btnVoyage"
                                                Width="28"
                                                Height="28"
                                                Margin="3,2,3,1"
                                                BackImage="/VesselTracking;component/Images/1329155031_ui-menu-blue.png"
                                                IsToggle="True"
                                                PreClicked="btnVoyage_PreClicked"
                                                Visibility="Collapsed" />
            <iDeal_silverlight_controls:CButton x:Name="btnStatic"
                                                Width="28"
                                                Height="28"
                                                Margin="3,0"
                                                BackImage="/VesselTracking;component/Images/1304585228_Windows_Table.png"
                                                IsToggle="True"
                                                PreClicked="btnStatic_PreClicked"
                                                Visibility="Collapsed" />
            <iDeal_silverlight_controls:CButton x:Name="btnTime"
                                                Width="28"
                                                Height="28"
                                                Margin="3,1.5"
                                                BackImage="/VesselTracking;component/Images/1326533559_clock.png"
                                                IsToggle="True"
                                                PreClicked="btnTime_PreClicked"
                                                Visibility="Collapsed" />
            <iDeal_silverlight_controls:CButton x:Name="btnHistory_Copy1"
                                                Width="28"
                                                Height="28"
                                                Margin="3,2,5,1"
                                                BackImage="/VesselTracking;component/Images/addtogroup.png"
                                                Clicked="btnAddToGroup_Clicked"
                                                ToolTipService.ToolTip="Add selected to a group" Visibility="{Binding AISType, ConverterParameter=A\,B, Converter={StaticResource ConverterAISTypeVisibility}}" />
            <iDeal_silverlight_controls:CButton x:Name="btnHistory_Copy2"
                                                Width="28"
                                                Height="28"
                                                Margin="3,2,5,1"
                                                BackImage="/VesselTracking;component/Images/mail_send.png"
                                                Clicked="btnSendMessage_Clicked"
                                                ToolTipService.ToolTip="Send text message to selected" Visibility="{Binding AISType, ConverterParameter=A\,B, Converter={StaticResource ConverterAISTypeVisibility}}"/>
            <iDeal_silverlight_controls:CButton x:Name="btnHistory_Copy3"
                                                Width="28"
                                                Height="28"
                                                Margin="3,2,5,1"
                                                BackImage="/VesselTracking;component/Images/mail_send.png"
                                                Clicked="btnBroadcastMessage_Clicked"
                                                ToolTipService.ToolTip="Broadcast a text message" Visibility="{Binding AISType, ConverterParameter=NODE, Converter={StaticResource ConverterAISTypeVisibility}}"/>
        </StackPanel>
        <Grid x:Name="gridIdentity"
              Height="65"
              Margin="10,13,9,0"
              VerticalAlignment="Top">
            <TextBlock x:Name="txtName1"
                       Height="24"
                       Margin="8,6,0,0"
                       VerticalAlignment="Top"
                       Cursor="Hand"
                       FontFamily="Lucida Sans Unicode"
                       FontSize="16"
                       Foreground="Yellow"
                       MouseLeftButtonUp="txtName_MouseLeftButtonUp"
                       Text="{Binding Name,
                                      FallbackValue=SHIP NAME}"
                       TextWrapping="Wrap">
                <TextBlock.Effect>
                    <DropShadowEffect ShadowDepth="2" />
                </TextBlock.Effect>
            </TextBlock>
            <Grid x:Name="mmsiGrid1"
                  Width="70"
                  Margin="8,31,0,0"
                  HorizontalAlignment="Left">
                <TextBlock x:Name="textBlock2"
                           Margin="0,0,0,11"
                           Style="{StaticResource InfoLabel}"
                           Text="MMSI"
                           TextWrapping="Wrap" />
                <TextBlock x:Name="textBlock_Copy40"
                           Style="{StaticResource InfoText}"
                           Text="{Binding MMSI,
                                          FallbackValue=-}"
                           TextWrapping="Wrap" />
            </Grid>
            <Grid x:Name="callsignGrid1"
                  Width="56"
                  Margin="78,31,0,0"
                  HorizontalAlignment="Left" Visibility="{Binding AISType, ConverterParameter=A\,B, Converter={StaticResource ConverterAISTypeVisibility}}">
                <TextBlock x:Name="textBlock_Copy38"
                           HorizontalAlignment="Stretch"
                           Style="{StaticResource InfoLabel}"
                           Text="Call Sign"
                           TextWrapping="Wrap"
                           d:LayoutOverrides="Width" />
                <TextBlock x:Name="textBlock_Copy39"
                           Style="{StaticResource InfoText}"
                           Text="{Binding CallSign,
                                          FallbackValue=-}"
                           TextWrapping="Wrap"
                           d:LayoutOverrides="HorizontalAlignment" />
            </Grid>
            <Grid x:Name="imoGrid1"
                  Width="69"
                  Margin="138,31,0,0"
                  HorizontalAlignment="Left" Visibility="{Binding AISType, ConverterParameter=A\,B, Converter={StaticResource ConverterAISTypeVisibility}}">
                <TextBlock x:Name="textBlock_Copy41"
                           HorizontalAlignment="Stretch"
                           Style="{StaticResource InfoLabel}"
                           Text="IMO"
                           TextWrapping="Wrap"
                           d:LayoutOverrides="Width" />
                <TextBlock x:Name="textBlock_Copy42"
                           Style="{StaticResource InfoText}"
                           Text="{Binding IMO,
                                          FallbackValue=-}"
                           TextWrapping="Wrap" />
            </Grid>
        </Grid>
        <c1:C1TabControl x:Name="c1TabControl"
                         Height="194"
                         Margin="5,80,9,0"
                         VerticalAlignment="Top"
                         FontSize="9"
                         RenderTransformOrigin="0.5,0.5"
                         SelectedIndex="5"
                         TabItemShape="Sloped"
                         TabStripPlacement="Right">
            <c1:C1TabControl.RenderTransform>
                <CompositeTransform />
            </c1:C1TabControl.RenderTransform>
            <c1:C1TabControl.SelectedBackground>
                <SolidColorBrush Color="{StaticResource Blue}" />
            </c1:C1TabControl.SelectedBackground>
            <c1:C1TabControl.Background>
                <SolidColorBrush Color="{StaticResource Blue}" />
            </c1:C1TabControl.Background>
            <c1:C1TabItem x:Name="tiVoyage"
                          Foreground="White"
                          Header="Voyage"
                          Style="{StaticResource C1TabItemStyle1}" Visibility="{Binding AISType, ConverterParameter=A\,B, Converter={StaticResource ConverterAISTypeVisibility}}">
                <c1:C1TabItem.MouseOverBrush>
                    <SolidColorBrush Color="{StaticResource LightBlue}" />
                </c1:C1TabItem.MouseOverBrush>
                <c1:C1TabItem.PressedBrush>
                    <SolidColorBrush Color="{StaticResource LightBlue}" />
                </c1:C1TabItem.PressedBrush>
                <c1:C1TabItem.Background>
                    <SolidColorBrush Color="{StaticResource DarkBlue}" />
                </c1:C1TabItem.Background>
                <c1:C1TabItem.SelectedBackground>
                    <SolidColorBrush Color="{StaticResource Blue}" />
                </c1:C1TabItem.SelectedBackground>
                <Grid x:Name="gridVoyage">
                    <StackPanel Height="36"
                                Margin="0"
                                VerticalAlignment="Top"
                                Orientation="Horizontal"
                                d:LayoutOverrides="Width">
                        <Grid x:Name="SOGGrid" Width="46">
                            <TextBlock x:Name="textBlock_Copy10"
                                       Width="41"
                                       Style="{StaticResource InfoLabel}"
                                       Text="SOG"
                                       TextWrapping="Wrap" />
                            <TextBlock x:Name="textBlock_Copy11"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding LabelSOG,
                                                      FallbackValue=-}"
                                       TextWrapping="Wrap"
                                       d:LayoutOverrides="HorizontalMargin" />
                        </Grid>
                        <Grid x:Name="COGGrid"
                              Width="46"
                              Margin="0">
                            <TextBlock x:Name="textBlock_Copy12"
                                       Width="41"
                                       Style="{StaticResource InfoLabel}"
                                       Text="COG"
                                       TextWrapping="Wrap" />
                            <TextBlock x:Name="textBlock_Copy13"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding COG,
                                                      ConverterParameter=# °,
                                                      Converter={StaticResource ConverterDouble},
                                                      FallbackValue=-}"
                                       TextWrapping="Wrap"
                                       d:LayoutOverrides="HorizontalMargin" />
                        </Grid>
                        <Grid x:Name="HDGGrid"
                              Width="46"
                              Margin="0"
                              HorizontalAlignment="Left">
                            <TextBlock x:Name="textBlock_Copy14"
                                       Width="41"
                                       Style="{StaticResource InfoLabel}"
                                       Text="HDG"
                                       TextWrapping="Wrap" />
                            <TextBlock x:Name="textBlock_Copy15"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding HDG,
                                                      ConverterParameter=# °,
                                                      Converter={StaticResource ConverterDouble},
                                                      FallbackValue=-}"
                                       TextWrapping="Wrap"
                                       d:LayoutOverrides="HorizontalMargin" />
                        </Grid>
                        <Grid x:Name="ROTGrid"
                              Width="46"
                              Margin="0"
                              HorizontalAlignment="Left">
                            <TextBlock x:Name="textBlock_Copy16"
                                       HorizontalAlignment="Stretch"
                                       Style="{StaticResource InfoLabel}"
                                       Text="ROT"
                                       TextWrapping="Wrap"
                                       d:LayoutOverrides="Width" />
                            <TextBlock x:Name="textBlock_Copy17"
                                       Width="42"
                                       Margin="0,14,0,0"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding ROT,
                                                      FallbackValue=-}"
                                       TextWrapping="Wrap" />
                        </Grid>
                    </StackPanel>
                    <StackPanel Margin="0,41,0,62"
                                Orientation="Vertical"
                                d:LayoutOverrides="Height">
                        <Grid x:Name="longitudeGrid"
                              Height="36"
                              VerticalAlignment="Top">
                            <TextBlock x:Name="textBlock1"
                                       Margin="0,0,29,11"
                                       Style="{StaticResource InfoLabel}"
                                       Text="Location"
                                       TextWrapping="Wrap" />
                            <TextBlock x:Name="textBlock_Copy18"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding LatLong,
                                                      Converter={StaticResource ConverterLatLong},
                                                      FallbackValue=-}"
                                       TextWrapping="Wrap" />
                        </Grid>
                    </StackPanel>
                    <StackPanel Height="36"
                                Margin="0,0,0,10"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Orientation="Horizontal">
                        <Grid x:Name="monthGrid"
                              Width="30"
                              Height="36"
                              Margin="0,0,16,0"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Top">
                            <TextBlock x:Name="textBlock_Copy25"
                                       HorizontalAlignment="Stretch"
                                       Style="{StaticResource InfoLabel}"
                                       Text="Month"
                                       TextWrapping="Wrap" />
                            <TextBlock x:Name="textBlock_Copy26"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding ETAMonth,
                                                      FallbackValue=-}"
                                       TextWrapping="Wrap" />
                        </Grid>
                        <Grid x:Name="dayGrid"
                              Width="24"
                              Height="36"
                              Margin="0,0,16,0"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Top">
                            <TextBlock x:Name="textBlock_Copy27"
                                       HorizontalAlignment="Stretch"
                                       Style="{StaticResource InfoLabel}"
                                       Text="Day"
                                       TextWrapping="Wrap" />
                            <TextBlock x:Name="textBlock_Copy28"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding ETADay,
                                                      FallbackValue=-}"
                                       TextWrapping="Wrap"
                                       d:LayoutOverrides="HorizontalMargin" />
                        </Grid>
                        <Grid x:Name="hourGrid"
                              Width="24"
                              Height="36"
                              Margin="0,0,16,0"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Top">
                            <TextBlock x:Name="textBlock_Copy29"
                                       HorizontalAlignment="Stretch"
                                       Style="{StaticResource InfoLabel}"
                                       Text="Hour"
                                       TextWrapping="Wrap" />
                            <TextBlock x:Name="textBlock_Copy30"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding ETAHour,
                                                      FallbackValue=-}"
                                       TextWrapping="Wrap" />
                        </Grid>
                    </StackPanel>
                    <StackPanel Height="36"
                                Margin="0,84,0,0"
                                VerticalAlignment="Top"
                                Orientation="Horizontal"
                                d:LayoutOverrides="Width">
                        <Grid x:Name="destinationGrid"
                              Width="92"
                              Height="36"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Top">
                            <TextBlock x:Name="textBlock_Copy22"
                                       HorizontalAlignment="Stretch"
                                       Style="{StaticResource InfoLabel}"
                                       Text="Destination"
                                       TextWrapping="Wrap" />
                            <TextBlock x:Name="textBlock_Copy23"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding Destination,
                                                      FallbackValue=-}"
                                       TextWrapping="Wrap" />
                        </Grid>
                    </StackPanel>
                </Grid>
            </c1:C1TabItem>
            <c1:C1TabItem x:Name="tiStatic"
                          Foreground="White"
                          Header="Static"
                          Style="{StaticResource C1TabItemStyle1}" Visibility="{Binding AISType, ConverterParameter=A\,B, Converter={StaticResource ConverterAISTypeVisibility}}">
                <c1:C1TabItem.MouseOverBrush>
                    <SolidColorBrush Color="{StaticResource LightBlue}" />
                </c1:C1TabItem.MouseOverBrush>
                <c1:C1TabItem.PressedBrush>
                    <SolidColorBrush Color="{StaticResource LightBlue}" />
                </c1:C1TabItem.PressedBrush>
                <c1:C1TabItem.Background>
                    <SolidColorBrush Color="{StaticResource DarkBlue}" />
                </c1:C1TabItem.Background>
                <c1:C1TabItem.SelectedBackground>
                    <SolidColorBrush Color="{StaticResource Blue}" />
                </c1:C1TabItem.SelectedBackground>
                <Grid x:Name="gridStatic"
                      Height="173"
                      VerticalAlignment="Top"
                      d:LayoutOverrides="HorizontalMargin">
                    <StackPanel Height="36"
                                Margin="0"
                                VerticalAlignment="Top"
                                Orientation="Horizontal"
                                d:LayoutOverrides="Width">
                        <Grid x:Name="shipTypeGrid1"
                              Width="128"
                              Height="36"
                              VerticalAlignment="Top">
                            <TextBlock x:Name="textBlock6"
                                       HorizontalAlignment="Stretch"
                                       Style="{StaticResource InfoLabel}"
                                       Text="Ship Type"
                                       TextWrapping="Wrap" />
                            <TextBlock x:Name="textBlock_Copy52"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding ShipAndCargo,
                                                      Converter={StaticResource ConverterShipAndCargo},
                                                      FallbackValue=N/A}"
                                       TextWrapping="Wrap"
                                       ToolTipService.ToolTip="{Binding ShipAndCargo,
                                                                        Converter={StaticResource ConverterShipAndCargoForTooltip}}" />
                        </Grid>
                        <Grid x:Name="aisTypeGrid1"
                              Width="43"
                              Height="36"
                              VerticalAlignment="Top">
                            <TextBlock x:Name="textBlock7"
                                       Width="43"
                                       Style="{StaticResource InfoLabel}"
                                       Text="AIS Type"
                                       TextWrapping="Wrap" />
                            <TextBlock x:Name="textBlock_Copy53"
                                       Width="43"
                                       HorizontalAlignment="Left"
                                       Style="{StaticResource InfoText}"
                                       Text="{Binding AISType,
                                                      FallbackValue=-}"
                                       TextAlignment="Center"
                                       TextWrapping="Wrap" />
                        </Grid>
                    </StackPanel>
                    <Grid x:Name="widthGrid1"
                          Width="40"
                          Margin="0,56,0,85"
                          HorizontalAlignment="Left"
                          d:LayoutOverrides="HorizontalAlignment">
                        <TextBlock x:Name="textBlock_Copy62"
                                   Margin="0"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Top"
                                   Style="{StaticResource InfoLabel}"
                                   Text="Width"
                                   TextAlignment="Right"
                                   TextWrapping="Wrap" />
                        <TextBlock x:Name="textBlock_Copy63"
                                   Margin="0,14,0,0"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Top"
                                   Style="{StaticResource InfoText}"
                                   Text="{Binding Width,
                                                  ConverterParameter=0 m,
                                                  Converter={StaticResource ConverterInt},
                                                  FallbackValue=-}"
                                   TextAlignment="Right"
                                   TextWrapping="Wrap" />
                    </Grid>
                    <Grid x:Name="lengthGrid1"
                          Width="40"
                          Height="32"
                          Margin="65,56,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top">
                        <TextBlock x:Name="textBlock_Copy64"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Top"
                                   Style="{StaticResource InfoLabel}"
                                   Text="Length"
                                   TextAlignment="Right"
                                   TextWrapping="Wrap" />
                        <TextBlock x:Name="textBlock_Copy65"
                                   Margin="0,14,0,0"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Top"
                                   Style="{StaticResource InfoText}"
                                   Text="{Binding Length,
                                                  ConverterParameter=0 m,
                                                  Converter={StaticResource ConverterInt},
                                                  FallbackValue=-}"
                                   TextAlignment="Right"
                                   TextWrapping="Wrap" />
                    </Grid>
                    <Grid x:Name="dimensionGrid1"
                          Width="133"
                          Height="51"
                          Margin="19,104,0,0"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Top">
                        <Image x:Name="image1"
                               Margin="4,0,0,2"
                               Opacity="0.535"
                               Source="../Images/ship4.png"
                               Stretch="Fill" />
                        <Ellipse Width="6"
                                 Margin="0,20,0,25"
                                 HorizontalAlignment="Center"
                                 Fill="#FF151516"
                                 Opacity="0.335" />
                        <TextBlock x:Name="textBlock_Copy34"
                                   Margin="0,16,86,19"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Stretch"
                                   FontFamily="Trebuchet MS"
                                   Style="{StaticResource InfoText}"
                                   Text="{Binding DimA,
                                                  FallbackValue=-}"
                                   TextAlignment="Right"
                                   TextWrapping="Wrap" />
                        <TextBlock x:Name="textBlock_Copy35"
                                   Margin="0,16,40,19"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Stretch"
                                   FontFamily="Trebuchet MS"
                                   Style="{StaticResource InfoText}"
                                   Text="{Binding DimB,
                                                  FallbackValue=-}"
                                   TextWrapping="Wrap" />
                        <TextBlock x:Name="textBlock_Copy36"
                                   Margin="64,3,64,0"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Top"
                                   FontFamily="Trebuchet MS"
                                   Style="{StaticResource InfoText}"
                                   Text="{Binding DimC,
                                                  FallbackValue=-}"
                                   TextAlignment="Right"
                                   TextWrapping="Wrap"
                                   d:LayoutOverrides="Width" />
                        <TextBlock x:Name="textBlock_Copy37"
                                   Margin="0,0,64,8"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Bottom"
                                   FontFamily="Trebuchet MS"
                                   Style="{StaticResource InfoText}"
                                   Text="{Binding DimD,
                                                  FallbackValue=-}"
                                   TextAlignment="Right"
                                   TextWrapping="Wrap" />
                    </Grid>
                    <Grid x:Name="draughtGrid"
                          Width="40"
                          Height="32"
                          Margin="0,56,8,0"
                          HorizontalAlignment="Right"
                          VerticalAlignment="Top">
                        <TextBlock x:Name="textBlock_Copy66"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Top"
                                   Style="{StaticResource InfoLabel}"
                                   Text="Draught"
                                   TextWrapping="Wrap" />
                        <TextBlock x:Name="textBlock_Copy67"
                                   Margin="0,14,0,0"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Top"
                                   Style="{StaticResource InfoText}"
                                   Text="{Binding Draught,
                                                  ConverterParameter=0 m,
                                                  Converter={StaticResource ConverterInt},
                                                  FallbackValue=-}"
                                   TextAlignment="Center"
                                   TextWrapping="Wrap" />
                    </Grid>
                </Grid>
            </c1:C1TabItem>
