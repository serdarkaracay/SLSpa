<UserControl x:Class="VesselTracking.Controls.CColumnSelectControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="248"
             d:DesignWidth="380"
             mc:Ignorable="d">

    <Grid x:Name="LayoutRoot">
        <ListBox Name="listBox1"
                 Width="134"
                 Height="173"
                 Margin="22,53,0,0"
                 HorizontalAlignment="Left"
                 VerticalAlignment="Top"
                 Background="#FF4F4F4F"
                 Foreground="White"
                 MouseLeftButtonUp="listBox1_MouseLeftButtonDown" />
        <Button Name="button1"
                Width="27"
                Height="28"
                Margin="162,53,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="button1_Click"
                Content="→" />
        <Button Name="button2"
                Width="27"
                Height="28"
                Margin="162,87,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="button2_Click"
                Content="←" />
        <ListBox Name="listBox2"
                 Width="134"
                 Height="173"
                 Margin="195,53,0,0"
                 HorizontalAlignment="Left"
                 VerticalAlignment="Top"
                 Background="#FF4F4F4F"
                 Foreground="White"
                 MouseLeftButtonUp="listBox2_MouseLeftButtonDown" />
        <Button Name="button3"
                Width="28"
                Height="28"
                Margin="335,53,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="button3_Click"
                Content="↑" />
        <Button Name="button4"
                Width="28"
                Height="28"
                Margin="335,87,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Background="Black"
                Click="button4_Click"
                Content="↓" />
        <TextBlock Name="textBlock1"
                   Width="134"
                   Height="19"
                   Margin="22,35,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Foreground="White"
                   Text="Available Columns" />
        <TextBlock Name="textBlock2"
                   Width="134"
                   Height="19"
                   Margin="195,35,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Foreground="White"
                   Text="Selected Columns" />
    </Grid>
</UserControl>
