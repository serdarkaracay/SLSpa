<UserControl x:Class="VesselTracking.Controls.CFilterControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="218"
             d:DesignWidth="487"
             mc:Ignorable="d">

    <Grid x:Name="LayoutRoot">
        <CheckBox Name="checkBox1"
                  Height="16"
                  Margin="82,13,0,0"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  Checked="checkBox1_Checked"
                  Content="All"
                  Foreground="White"
                  IsChecked="True"
                  Unchecked="checkBox1_UnChecked" />
        <CheckBox Name="checkBox2"
                  Height="16"
                  Margin="82,35,104,0"
                  VerticalAlignment="Top"
                  Checked="checkBox2_Checked"
                  Content="Class A"
                  Foreground="White"
                  Unchecked="checkBox2_UnChecked"
                  d:LayoutOverrides="Width" />
        <CheckBox Name="checkBox3"
                  Height="16"
                  Margin="82,57,104,0"
                  VerticalAlignment="Top"
                  Checked="checkBox3_Checked"
                  Content="Class B"
                  Foreground="White"
                  Unchecked="checkBox3_UnChecked"
                  d:LayoutOverrides="Width" />
        <CheckBox Name="checkBox4"
                  Height="16"
                  Margin="82,79,118,0"
                  VerticalAlignment="Top"
                  Checked="checkBox4_Checked"
                  Content="AtoN"
                  Foreground="White"
                  Unchecked="checkBox4_UnChecked"
                  d:LayoutOverrides="Width" />
        <CheckBox Name="checkBox5"
                  Height="16"
                  Margin="82,101,75,0"
                  VerticalAlignment="Top"
                  Checked="checkBox5_Checked"
                  Content="Base Station"
                  Foreground="White"
                  Unchecked="checkBox5_UnChecked"
                  d:LayoutOverrides="Width" />
        <CheckBox Name="checkBox6"
                  Height="16"
                  Margin="82,123,89,0"
                  VerticalAlignment="Top"
                  Checked="checkBox6_Checked"
                  Content="Undefined"
                  Foreground="White"
                  Unchecked="checkBox6_UnChecked"
                  d:LayoutOverrides="Width" />
        <TextBox Name="textBox1"
                 Width="148"
                 Height="32"
                 Margin="256,57,0,0"
                 HorizontalAlignment="Left"
                 VerticalAlignment="Top" />
    </Grid>
</UserControl>
