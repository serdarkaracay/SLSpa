                                                                Height="32"
                                                                Margin="2,1"
                                                                HorizontalAlignment="Right"
                                                                BackImage="/VesselTracking;component/Images/search.png"
                                                                RenderTransformOrigin="0.656,0.552"
                                                                ToolTipService.ToolTip="Filter the data" />
                            <iDeal_silverlight_controls:CButton x:Name="btnGridColumns"
                                                                Width="32"
                                                                Height="32"
                                                                Margin="2,1"
                                                                HorizontalAlignment="Right"
                                                                BackImage="/VesselTracking;component/Images/column.png"
                                                                RenderTransformOrigin="0.656,0.552"
                                                                ToolTipService.ToolTip="Add/remove columns on the grid" />
                        </StackPanel>
