<vtPages:AuthenticatedPage x:Class="VesselTracking.Pages.pageSettings"
                           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                           xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
                           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                           xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
                           xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
                           xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
                           xmlns:vtPages="clr-namespace:VesselTracking.Pages"
                           Title="Settings"
                           d:DesignHeight="607"
                           d:DesignWidth="861"
                           mc:Ignorable="d">
    <Grid x:Name="LayoutRoot">
        <c1:C1TabControl x:Name="c1TabControl"
                         Margin="0"
                         RenderTransformOrigin="0.5,0.5"
                         SelectedIndex="2"
                         TabItemShape="Sloped"
                         TabStripPlacement="Right">
            <c1:C1TabControl.RenderTransform>
                <CompositeTransform />
            </c1:C1TabControl.RenderTransform>
            <c1:C1TabControl.SelectedBackground>
                <SolidColorBrush Color="{StaticResource LightBlue}" />
            </c1:C1TabControl.SelectedBackground>
            <c1:C1TabControl.Background>
                <SolidColorBrush Color="{StaticResource LightBlue}" />
            </c1:C1TabControl.Background>
            <c1:C1TabItem x:Name="tiGeneral"
                          Foreground="White"
                          Header="General"
                          Style="{StaticResource C1TabItemStyle1}">
                <c1:C1TabItem.MouseOverBrush>
                    <SolidColorBrush Color="{StaticResource LightBlue}" />
                </c1:C1TabItem.MouseOverBrush>
                <c1:C1TabItem.PressedBrush>
                    <SolidColorBrush Color="{StaticResource LightBlue}" />
                </c1:C1TabItem.PressedBrush>
                <c1:C1TabItem.Background>
                    <SolidColorBrush Color="{StaticResource DarkBlue}" />
                </c1:C1TabItem.Background>
                <c1:C1TabItem.SelectedBackground>
                    <SolidColorBrush Color="{StaticResource LightBlue}" />
                </c1:C1TabItem.SelectedBackground>
                <Grid>
                	<ScrollViewer BorderBrush="{x:Null}" ScrollViewer.VerticalScrollBarVisibility="Auto" Style="{StaticResource ScrollViewerStyle1}">
                		<toolkit:WrapPanel Orientation="Horizontal" ScrollViewer.VerticalScrollBarVisibility="Auto" MinHeight="480" MinWidth="600">
                			<Grid Width="300" Height="240" Margin="20" VerticalAlignment="Top" HorizontalAlignment="Left">
                				<Rectangle Margin="0"
                					Fill="#FF104A74"
                					RadiusX="4"
                					RadiusY="4"
                					StrokeThickness="2">
                					<Rectangle.Effect>
                						<DropShadowEffect BlurRadius="30"
                							Opacity="0.5"
                							ShadowDepth="10" />
                					</Rectangle.Effect>
                					<Rectangle.Stroke>
                						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                							<GradientStop Offset="1" Color="#FF0A2F4B" />
                							<GradientStop Color="#FF1667A1" />
                						</LinearGradientBrush>
                					</Rectangle.Stroke>
                				</Rectangle>
                				<StackPanel Orientation="Vertical" d:LayoutOverrides="Height">
                					<Rectangle
                						Fill="#FF104A74"
                						RadiusX="4"
                						RadiusY="4"
                						StrokeThickness="2" Height="26" Margin="10,10,10,0">
                						<Rectangle.Stroke>
                							<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                								<GradientStop Color="#FF0A2F4B" />
                								<GradientStop Color="#FF1667A1" Offset="1" />
                							</LinearGradientBrush>
                						</Rectangle.Stroke>
                					</Rectangle>
                					<StackPanel Margin="10,0"
                						Orientation="Vertical"
                				>
                						<Grid x:Name="gridStaticData1"
                							Height="23"
                							Margin="0,9,0,0">
                							<sdk:Label x:Name="label15"
                								Height="18"
                								Margin="0,0,115,0"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Content="Static Data Period (sec)"
                								Foreground="#FFA0A0A0" />
                							<ComboBox x:Name="cmbStaticServicePeriod1"
                								Width="110"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								SelectionChanged="cmbStaticServicePeriod_SelectionChanged" />
                						</Grid>
                						<Grid x:Name="gridVoyageData1"
                							Height="23"
                							Margin="0,9,0,0">
                							<sdk:Label x:Name="label14"
                								Margin="0,0,115,0"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Content="Voyage Data Period (sec)"
                								Foreground="#FFA0A0A0" />
                							<ComboBox x:Name="cmbVoyageServicePeriod1"
                								Width="110"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								SelectionChanged="cmbRefreshPeriod_SelectionChanged" />
                						</Grid>
                						<Grid x:Name="gridSignToRemove1"
                							Height="23"
                							Margin="0,9,0,0">
                							<sdk:Label x:Name="label12"
                								Margin="0,0,115,0"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Content="Sign To Remove (sec)"
                								Foreground="#FFA0A0A0" />
                							<ComboBox x:Name="cmbSignToRemoveSecond1"
                								Width="110"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								SelectionChanged="cmbSignToRemoveSecond_SelectionChanged" />
                						</Grid>
                						<Grid x:Name="gridRemoveSecond1"
                							Height="23"
                							Margin="0,9,0,0">
                							<sdk:Label x:Name="label13"
                								Margin="0,0,115,0"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Content="Remove (sec)"
                								Foreground="#FFA0A0A0" />
                							<ComboBox x:Name="cmbRemoveSecond1"
                								Width="110"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								SelectionChanged="cmbRemoveSecond_SelectionChanged" />
                						</Grid>
                						<Grid x:Name="grdiTrackLine1"
                							Height="23"
                							Margin="0,9,0,0">
                							<sdk:Label x:Name="label16"
                								Height="18"
                								Margin="0,0,115,0"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Content="Track Line Period (min)"
                								Foreground="#FFA0A0A0" />
                							<ComboBox x:Name="cmbTrackLinePeriod1"
                								Width="110"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								SelectionChanged="cmbTrackLinePeriod_SelectionChanged" />
                						</Grid>
                						<Grid x:Name="gridStaticNodeData"
                							Height="23"
                							Margin="0,9,0,0">
                							<sdk:Label x:Name="label1"
                								Margin="0,0,115,0"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Content="Node Data Period (sec)"
                								Foreground="#FFA0A0A0" />
                							<ComboBox x:Name="cmbNodeServicePeriod"
                								Width="110"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center" SelectionChanged="cmbNodeServicePeriod_SelectionChanged" />
                						</Grid>
                					</StackPanel>
                				</StackPanel>
                				<TextBlock TextWrapping="Wrap" Text="Services Period" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="17,13,0,0" Foreground="#FFFFF58B" FontSize="13.333">
                					<TextBlock.Effect>
                						<DropShadowEffect ShadowDepth="1" BlurRadius="3"/>
                					</TextBlock.Effect>
                				</TextBlock>
                			</Grid>
                			<Grid VerticalAlignment="Top" HorizontalAlignment="Left" Height="102" Margin="20" Width="300">
                				<Rectangle
                					Fill="#FF104A74"
                					RadiusX="4"
                					RadiusY="4"
                					StrokeThickness="2">
                					<Rectangle.Effect>
                						<DropShadowEffect BlurRadius="30"
                							Opacity="0.5"
                							ShadowDepth="10" />
                					</Rectangle.Effect>
                					<Rectangle.Stroke>
                						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                							<GradientStop Offset="1" Color="#FF0A2F4B" />
                							<GradientStop Color="#FF1667A1" />
                						</LinearGradientBrush>
                					</Rectangle.Stroke>
                				</Rectangle>
                				<StackPanel Orientation="Vertical" d:LayoutOverrides="Height">
                					<Rectangle
                						Fill="#FF104A74"
                						RadiusX="4"
                						RadiusY="4"
                						StrokeThickness="2" Height="26" Margin="10,10,10,0">
                						<Rectangle.Stroke>
                							<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                								<GradientStop Color="#FF0A2F4B" />
                								<GradientStop Color="#FF1667A1" Offset="1" />
                							</LinearGradientBrush>
                						</Rectangle.Stroke>
                					</Rectangle>
                					<StackPanel Orientation="Vertical" Margin="0,0,10,0">
                						<Grid x:Name="gridBrightness1"
                							Height="18" Margin="0,10,0,0"
                    			>
                							<sdk:Label x:Name="label6_Copy1"
                								Width="110"
                								Margin="0,0,115,0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Foreground="#FFA0A0A0"
                								Content="Brightness" />
                							<Slider x:Name="sliderBrightness1"
                								Width="110"
                								Margin="0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								LostFocus="sliderBrightness_LostFocus"
                								Maximum="35"
                								Minimum="-35"
                								RenderTransformOrigin="0.5,0.5"
                								ValueChanged="sliderBrightness_ValueChanged" LargeChange="5" SmallChange="1"/>
                						</Grid>
                						<Grid x:Name="gridContrast1"
                							Height="18" Margin="0,10,0,0"
                    			>
                							<sdk:Label x:Name="label6_Copy3"
                								Width="110"
                								Margin="0,0,115,0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Foreground="#FFA0A0A0" 
                								Content="Contrast"/>
                							<Slider x:Name="sliderContrast1"
                								Width="110"
                								Margin="0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								LostFocus="sliderContrast_LostFocus"
                								Maximum="130"
                								RenderTransformOrigin="0.5,0.5"
                								ValueChanged="sliderContrast_ValueChanged" SmallChange="1" LargeChange="5" Minimum="70" Value="100" />
                						</Grid>
                					</StackPanel>
                				</StackPanel>
                				<TextBlock TextWrapping="Wrap" Text="Display" Margin="17,13,105,31" Foreground="#FFFFF58B" FontSize="13.333" d:LayoutOverrides="Width, Height">
                					<TextBlock.Effect>
                						<DropShadowEffect ShadowDepth="1" BlurRadius="3"/>
                					</TextBlock.Effect>
                				</TextBlock>
                			</Grid>
                			<Grid Width="300" Height="192" Margin="20" VerticalAlignment="Top" HorizontalAlignment="Left">
                				<Rectangle Margin="0"
                					Fill="#FF104A74"
                					RadiusX="4"
                					RadiusY="4"
                					StrokeThickness="2">
                					<Rectangle.Effect>
                						<DropShadowEffect BlurRadius="30"
                							Opacity="0.5"
                							ShadowDepth="10" />
                					</Rectangle.Effect>
                					<Rectangle.Stroke>
                						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                							<GradientStop Offset="1" Color="#FF0A2F4B" />
                							<GradientStop Color="#FF1667A1" />
                						</LinearGradientBrush>
                					</Rectangle.Stroke>
                				</Rectangle>
                				<StackPanel Orientation="Vertical" d:LayoutOverrides="Height">
                					<Rectangle
                						Fill="#FF104A74"
                						RadiusX="4"
                						RadiusY="4"
                						StrokeThickness="2" Height="26" Margin="10,10,10,0">
                						<Rectangle.Stroke>
                							<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                								<GradientStop Color="#FF0A2F4B" />
                								<GradientStop Color="#FF1667A1" Offset="1" />
                							</LinearGradientBrush>
                						</Rectangle.Stroke>
                					</Rectangle>
                					<StackPanel Margin="10,0"
                						Orientation="Vertical"
                				>
                						<Grid x:Name="gridDateFormat1"
                							Height="23"
                							Margin="0,9,0,0">
                							<sdk:Label x:Name="label17"
                								Margin="0,0,115,0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Content="Date Format"
                								Foreground="#FFA0A0A0" />
                							<ComboBox x:Name="cmbDateFormat1"
                								Width="110"
                								Margin="0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								SelectionChanged="cmbDateFormat1_SelectionChanged" />
                						</Grid>
                						<Grid x:Name="gridTimeFormat1"
                							Height="23"
                							Margin="0,9,0,0">
                							<sdk:Label x:Name="label18"
                								Width="110"
                								Margin="0,0,115,0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Content="Time Format"
                								Foreground="#FFA0A0A0" />
                							<ComboBox x:Name="cmbTimeFormat1"
                								Width="110"
                								Margin="0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								SelectionChanged="cmbTimeFormat1_SelectionChanged" />
                						</Grid>
                						<Grid x:Name="gridTimeZone1"
                							Height="23"
                							Margin="0,9,0,0">
                							<sdk:Label x:Name="label19"
                								Width="110"
                								Margin="0,0,115,0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Content="Time Zone"
                								Foreground="#FFA0A0A0" />
                							<ComboBox x:Name="cmbTimeZone1"
                								Width="110"
                								Margin="0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								SelectionChanged="cmbTimeZone1_SelectionChanged" />
                						</Grid>
                						<Grid x:Name="gridPositionFormat1"
                							Height="23"
                							Margin="0,9,0,0">
                							<sdk:Label x:Name="label20"
                								Width="110"
                								Margin="0,0,115,0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								HorizontalContentAlignment="Right"
                								Content="Geo Position Format"
                								Foreground="#FFA0A0A0" />
                							<ComboBox x:Name="cmbPositionFormat1"
                								Width="110"
                								Margin="0"
                								HorizontalAlignment="Right"
                								VerticalAlignment="Center"
                								SelectionChanged="cmbPositionFormat1_SelectionChanged" />
                						</Grid>
                					</StackPanel>
                				</StackPanel>
                				<TextBlock TextWrapping="Wrap" Text="Formats" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="17,13,0,0" Foreground="#FFFFF58B" FontSize="13.333">
                					<TextBlock.Effect>
                						<DropShadowEffect ShadowDepth="1" BlurRadius="3"/>
                					</TextBlock.Effect>
                				</TextBlock>
                			</Grid>
                		</toolkit:WrapPanel>
                	</ScrollViewer>
                </Grid>
            </c1:C1TabItem>
            <c1:C1TabItem x:Name="tiHelp"
                          Foreground="White"
                          Header="Help"
                          Style="{StaticResource C1TabItemStyle1}">
                <c1:C1TabItem.MouseOverBrush>
                    <SolidColorBrush Color="{StaticResource LightBlue}" />
                </c1:C1TabItem.MouseOverBrush>
                <c1:C1TabItem.PressedBrush>
                    <SolidColorBrush Color="{StaticResource LightBlue}" />
                </c1:C1TabItem.PressedBrush>
                <c1:C1TabItem.Background>
                    <SolidColorBrush Color="{StaticResource DarkBlue}" />
                </c1:C1TabItem.Background>
                <c1:C1TabItem.SelectedBackground>
                    <SolidColorBrush Color="{StaticResource LightBlue}" />
                </c1:C1TabItem.SelectedBackground>
                <Grid />

            </c1:C1TabItem>


        </c1:C1TabControl>
    </Grid>
</vtPages:AuthenticatedPage>
