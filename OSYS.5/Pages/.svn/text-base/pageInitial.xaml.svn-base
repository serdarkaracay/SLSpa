<vtClasses:AuthenticatedPage x:Class="VesselTracking.Pages.pageInitial"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
                             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                             xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
                             xmlns:VesselTracking="clr-namespace:VesselTracking"
                             xmlns:vtClasses="clr-namespace:VesselTracking.Pages"
                             Title="Welcome"
                             d:DesignHeight="480"
                             d:DesignWidth="640"
                             mc:Ignorable="d">
    <Grid x:Name="LayoutRoot">
        <Grid Width="208"
              Height="89"
              Margin="0,0,4,6"
              HorizontalAlignment="Right"
              VerticalAlignment="Bottom">
            <Image x:Name="image"
                   Margin="4,25,4,18"
                   RenderTransformOrigin="0.5,0.5"
                   Source="../Images/iDealLogoTransparent.png">
                <Image.Effect>
                    <DropShadowEffect />
                </Image.Effect>
            </Image>
            <TextBlock x:Name="textBlock1_Copy"
                       Width="207.1"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Bottom"
                       FontFamily="/VesselTracking;component/Fonts/Fonts.zip#AR BERKLEY"
                       FontSize="14"
                       Text="the Art of Engineering"
                       TextAlignment="Center"
                       TextWrapping="Wrap">
                <TextBlock.Effect>
                    <DropShadowEffect ShadowDepth="2" />
                </TextBlock.Effect>
                <TextBlock.Foreground>
                    <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                        <GradientStop Color="#FF1A8991" />
                        <GradientStop Offset="1" Color="#FF0F3F43" />
                    </LinearGradientBrush>
                </TextBlock.Foreground>
            </TextBlock>
        </Grid>
        <Viewbox Margin="0,10,0,120">
            <Grid Width="361" Height="350.025001525879">
                <VesselTracking:logo Width="134"
                                     Margin="0,67,0,0"
                                     HorizontalAlignment="Center"
                                     VerticalAlignment="Top">
                    <VesselTracking:logo.Effect>
                        <DropShadowEffect BlurRadius="10" />
                    </VesselTracking:logo.Effect>
                </VesselTracking:logo>
                <TextBlock x:Name="textBlock"
                           Width="361"
                           Margin="0,163,0,0"
                           HorizontalAlignment="Center"
                           Effect="{StaticResource SoftwareNameEffect}"
                           FontFamily="/VesselTracking;component/Fonts/Fonts.zip#Copperplate Gothic Bold"
                           FontSize="56"
                           FontWeight="Bold"
                           Foreground="{StaticResource SoftwareNameBrush}"
                           Text="Vessel Tracking System"
                           TextAlignment="Center"
                           TextWrapping="Wrap"
                           d:LayoutOverrides="Height" />
            </Grid>
        </Viewbox>
    </Grid>
</vtClasses:AuthenticatedPage>
