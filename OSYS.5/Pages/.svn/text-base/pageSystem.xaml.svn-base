                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                    <GradientStop Color="#FF00487C" />
                                    <GradientStop Offset="1" Color="#FF005A9D" />
                                </LinearGradientBrush>

                            </Rectangle.Stroke>
                        </Rectangle>

                        <sdk:DataGrid x:Name="dataGridVessels"
                                      Margin="0,0,40,0"
                                      AutoGenerateColumns="False"
                                      ColumnWidth="*"
                                      IsReadOnly="True">
                            <sdk:DataGrid.Columns>

                                <sdk:DataGridTextColumn Binding="{Binding MMSI}"
                                                        CanUserResize="False"
                                                        CanUserSort="True"
                                                        Header="MMSI"
                                                        IsReadOnly="True"
                                                        SortMemberPath="MMSI" />
                                <sdk:DataGridTextColumn MinWidth="20"
                                                        Binding="{Binding Name}"
                                                        CanUserSort="True"
                                                        Header="Name"
                                                        IsReadOnly="True"
                                                        SortMemberPath="Name" />
                                <sdk:DataGridTextColumn Binding="{Binding Location}"
                                                        CanUserSort="True"
                                                        Header="Position"
                                                        IsReadOnly="True"
                                                        SortMemberPath="Location" />
                                <sdk:DataGridTextColumn Binding="{Binding ReportDate,
                                                                          ConverterParameter=dd.MM.yy HH:mm:ss,
                                                                          Converter={StaticResource ConverterDateTime},
                                                                          FallbackValue=-}"
                                                        CanUserSort="True"
                                                        Header="Report Date"
                                                        IsReadOnly="True"
                                                        SortMemberPath="ReportDate" />
                                <sdk:DataGridTextColumn Binding="{Binding ReportPeriodSecond,
                                                                          ConverterParameter=# sec,
                                                                          Converter={StaticResource ConverterInt},
                                                                          FallbackValue=-}"
                                                        CanUserSort="True"
                                                        Header="Report Period"
                                                        IsReadOnly="True"
                                                        SortMemberPath="ReportPeriodSecond" />
                                <sdk:DataGridTextColumn Header="Status" />
                                <sdk:DataGridTextColumn MinWidth="0"
                                                        MaxWidth="Infinity"
                                                        Binding="{x:Null}"
                                                        CanUserReorder="True"
                                                        CanUserResize="True"
                                                        CanUserSort="True"
                                                        CellStyle="{x:Null}"
                                                        ClipboardContentBinding="{x:Null}"
                                                        DisplayIndex="-1"
                                                        DragIndicatorStyle="{x:Null}"
                                                        EditingElementStyle="{x:Null}"
                                                        ElementStyle="{x:Null}"
                                                        FontSize="NaN"
                                                        FontStyle="Normal"
