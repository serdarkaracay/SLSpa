                            <iDeal_silverlight_controls:CButton x:Name="btnRuler"
                                                                Width="32"
                                                                Height="32"
                                                                Margin="2,1"
                                                                HorizontalAlignment="Right"
                                                                BackImage="/VesselTracking;component/Images/ruler.png"
                                                                IsToggle="True" />
                            <iDeal_silverlight_controls:CButton x:Name="btnRegion"
                                                                Width="32"
                                                                Height="32"
                                                                Margin="2,1"
                                                                HorizontalAlignment="Right"
                                                                BackImage="/VesselTracking;component/Images/region.png"
                                                                IsToggle="True" />

