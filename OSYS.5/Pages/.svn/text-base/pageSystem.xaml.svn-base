                                <iDeal_silverlight:CGroupButton.Buttons>
                                    <iDeal_silverlight_controls:CButton BackImage="/VesselTracking;component/Images/view.png" />
                                    <iDeal_silverlight_controls:CButton BackImage="/VesselTracking;component/Images/view.png" />
                                    <iDeal_silverlight_controls:CButton BackImage="/VesselTracking;component/Images/view.png" />
                                    <iDeal_silverlight_controls:CButton BackImage="/VesselTracking;component/Images/view.png" />
